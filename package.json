{
  "name": "@oomia/upside-cookie-land",
  "version": "2.0.0",
  "description": "Upside Cookie Land",
  "directories": {
    "lib": "lib",
    "test": "test",
    "script": "script"
  },
  "scripts": {
    "preinstall": "forge install && forge update && yarn run remapping",
    "test": "forge clean && forge coverage",
    "format": "yarn compile && prettier --write . && forge fmt",
    "build": "forge build",
    "compile": "forge compile",
    "remapping": "forge remappings > remappings.txt",
    "deploy": "forge script script/Deploy.s.sol:Deploy",
    "info": "cast client",
    "docs": "bash docs.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ooMia/Upside_Cookie_Land.git"
  },
  "keywords": [
    "upside"
  ],
  "author": "ooMia",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ooMia/Upside_Cookie_Land/issues"
  },
  "homepage": "https://github.com/ooMia/Upside_Cookie_Land#readme",
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-solidity": "^1.4.1"
  },
  "engines": {
    "node": ">=22.6.0"
  },
  "packageManager": "yarn@4.5.0"
}
