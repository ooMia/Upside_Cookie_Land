{
  "name": "@oomia/upside-cookie-land",
  "version": "0.0.1",
  "description": "Upside Cookie Land",
  "directories": {
    "lib": "lib",
    "test": "test",
    "script": "script",
    "proxy": "src/proxy"
  },
  "scripts": {
    "install": "yarn install --ignore-scripts && forge install && forge update && yarn run remapping ",
    "test": "npm run format && forge test || forge coverage",
    "format": "yarn compile && prettier --write . && forge fmt",
    "build": "forge build",
    "compile": "forge compile",
    "remapping": "forge remappings > remappings.txt",
    "deploy": "forge script script/Deploy.s.sol:Deploy",
    "info": "cast client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ooMia/Upside_Cookie_Land.git"
  },
  "keywords": [
    "upside"
  ],
  "author": "ooMia",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ooMia/Upside_Cookie_Land/issues"
  },
  "homepage": "https://github.com/ooMia/Upside_Cookie_Land#readme",
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-solidity": "^1.4.1"
  },
  "engines": {
    "node": ">=22.6.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
